    "@tanstack/react-query": "^4.36.1",
    "@tanstack/react-query-devtools": "^4.36.1",
    "axios": "^1.5.1",
    "framer-motion": "^10.16.4",
    "msw": "^1.3.2",
    "next": "13.5.4",
    "react": "^18",
    "react-dom": "^18",
    "tinycolor2": "^1.6.0"

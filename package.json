    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.5",
    "@types/node": "^20",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18",
    "@types/tinycolor2": "^1.4.4",
    "encoding": "^0.1.13",
    "eslint": "^8",
    "eslint-config-next": "13.5.4",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.0.1",
    "prettier": "^3.0.3",
    "storybook": "^7.5.2",
    "typescript": "^5"
